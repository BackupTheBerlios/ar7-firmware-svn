--- package/base-files/files/etc/init.d/settime	(Revision 0)
+++ package/base-files/files/etc/init.d/settime	(Revision 0)
@@ -0,0 +1,13 @@
+#!/bin/sh /etc/rc.common
+
+# Set the time. Use file timestamp from NFS file.
+
+START=05
+
+start() {
+  touch /tmp/nfsfile
+  rm /tmp/nfsfile
+  date `date -r /tmp +%m%d%H%M%Y.%S`
+}
+
+# eof

